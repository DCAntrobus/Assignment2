<div class="contactContainer">
    <h1 class="contact">Contact Me</h1>
    <br>
    <!--Form -->
    <form action="">
        <label for="firstName">First Name:</label>
        <input type="text" id="firstName" name="firstName">
        <br>
        <label for="lastName">Last Name:</label>
        <input type="text" id="lastName" name="lastName">
        <br>
        <label for="email">Email Address:</label>
        <input type="email" id="email" name="email">
        <br>
        <label for="Contact">Phone Number:</label>
        <input type="tel" id="Contact" name="Contact" pattern="[0-9]{3}-[0-9]{3}-[0-9]{3}">
        <br>
        <label for="Message">Message</label>
        <br>
        <textarea class="Message" name="Message" rows="5" cols="100"></textarea>
        <br>
        <!-- This button leads back to home page-->
        <a href="home" class="btn btn-dark">Submit </a>
        <div class="links">
            <a href="https://www.linkedin.com/in/desmond-antrobus-0b4861105/"><img src="/content/images/linked-in.png"
                    alt="linkedin"></a>
        </div>


</div>

</div>